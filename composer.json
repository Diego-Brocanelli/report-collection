    },
    "autoload": {
        "psr-4": {
            "ReportCollection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReportCollection\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "ReportCollection\\ServiceProvider"
            ],
            "aliases": {
                "ReportCollection": "ReportCollection\\Facade"
            }
        }
    },
    "require-dev": {
        "illuminate/framework": "5.5.x"
    }
}
