{
    "name": "plexi/report-collection",
    "description": "Manipula qualquer lista de dados para serem exportadas em arquivos de relatÃ³rio",
    "keywords": ["laravel","report","collection","exporters","helpers", "csv", "xls", "xlsx", "ods", "html"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Pereira",
            "email": "rpdesignerfly@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "phpoffice/phpspreadsheet": "^1.2",
        "mpdf/mpdf": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
