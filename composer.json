        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "aliases": {
                "ReportCollection": "ReportCollection\\Facade"
            }
        }
    }
}
